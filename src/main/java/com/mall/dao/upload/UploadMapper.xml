<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mall.dao.upload.UploadMapper">
  <resultMap id="BaseResultMap" type="com.mall.generate.Upload">
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="path" jdbcType="VARCHAR" property="path" />
  </resultMap>
  <insert id="insert" parameterType="com.mall.generate.Upload">
    <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
      SELECT LAST_INSERT_ID()
    </selectKey>
    insert into upload (path)
    values (#{path,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select id, path
    from upload
  </select>

    <insert id="insertUpload"   >
      insert into  upload (path)value (#{path})
    </insert>
</mapper>